#!/usr/bin/env python3
# vim: tabstop=4 expandtab autoindent shiftwidth=4 fileencoding=utf-8 filetype=python

from ripwhine.interface import Interface

import sys


def main(args):
    interface = Interface()
    interface.run()
    return 0


if __name__ == '__main__':
    sys.exit(main(sys.argv))

